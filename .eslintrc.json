{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "import"
    ],
    "rules": {
        "no-console": "warn",
        "no-undef": "error",
        "no-duplicate-imports": "error",
        "import/extensions": [
            "error",
            "ignorePackages"
        ],
    }
}